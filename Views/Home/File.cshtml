@model FileMaster
@{
}

<div class="container">
    <div class="card">
        <div class="card-header">
            <div class="card-title"> Production </div>

        </div>
        <div class="card-body">
            <table id="filetable" class="display" style="width:100%">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>FileNumber</th>
                        <th>Country</th>
                        <th>Eta</th>
                        <th>DraftCutOff</th>
                        <th>HblCount</th>
                       
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>

        <div class="footer">
        </div>


    </div>
</div>
@section Scripts
    {
    <script>

        $(document).ready(function () {
            $('#filetable').DataTable({
                scrollY: '200px',
                scrollCollapse: true,
                paging: false,
                "ajax": {
                    "url": "@Url.Action("GetData","Home")",
                    "type": "GET",
                    "dataType": "json",
                    "dataSrc": ""  // Leave it empty if the returned data is already an array
                },
                "columns": [
                    { "data": "Id" },
                    { "data": "FileNumber" },
                    { "data": "Country" },
                    { "data": "Eta" },
                    { "data": "DraftCutOff" },
                    { "data": "HblCount" },

                    // Add more columns as needed
                ]
            });
        });

    </script>

}